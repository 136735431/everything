<% include header%>
<script src="/js/controller/productController.js"></script>
<div class="container" ng-app="myapp" ng-controller="productController">
    <div class="row">
        <table class="table table-striped  table-hover ">
            <thead>
            <tr>
                <td>ID</td>
                <td>名称</td>
                <td>属性1</td>
                <td>属性2</td>
                <td>时间</td>
                <td>操作</td>
            </tr>
            </thead>
            <tbody>
            <tr np-repeat=" x in products">
                <td ng-model="_id">{{x._id}}</td>
                <td ng-model="name">{{x.name}}</td>
                <td ng-model="attr1">{{x.attr1}}</td>
                <td ng-model="attr2">{{x.attr2}}</td>
                <td ng-model="date">{{x.date}}</td>
                <!--<td><a href="javascript:;" onclick="delData('{{x._id}}');" class="btn btn-warning">删除</a></td>-->
            </tr>

            </tbody>
        </table>
    </div>
</div>

<% include footer%>